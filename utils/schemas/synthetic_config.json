{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Observability Pack Synthetics Configuration",
  "description": "Synthetics config for Observability Packs",
  "type": "object",
  "properties": {
    "name": {
      "description": "A descriptive name for the Synthetic.",
      "type": "string"
    },
    "type": {
      "description": "The support level provided to this Observability Pack",
      "type": "string",
      "enum": ["New Relic", "Community Plus", "Community"]
    }
  },
  "required": [ "name", "level" ]
}
